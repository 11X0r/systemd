# do not edit this file, it will be overwritten on update

ACTION=="remove", GOTO="camera_end"

SUBSYSTEM=="video4linux", KERNEL=="video*", ENV{ID_BUS}!="", \
  IMPORT{builtin}="hwdb 'camera:$env{ID_BUS}:v$attr{id/vendor}p$attr{id/product}:name:$attr{name}:'", \
  GOTO="camera_end"

LABEL="camera_end"
