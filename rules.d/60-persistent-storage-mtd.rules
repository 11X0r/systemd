# do not edit this file, it will be overwritten on update

# persistent storage links: /dev/mtd/by-name

ACTION=="remove", GOTO="persistent_storage_mtd_end"
ENV{UDEV_DISABLE_PERSISTENT_STORAGE_MTD_FLAG}=="1", GOTO="persistent_storage_mtd_end"

SUBSYSTEM=="mtd", KERNEL=="mtd[0-9]*", ATTR{name}=="?*", SYMLINK+="mtd/by-name/$attr{name}"

LABEL="persistent_storage_mtd_end"
