[Unit]
Description=Test for ExecSearchPaths with EnvironmentFile where EnvironmentFile does not set PATH

[Service]
ExecStart=/bin/sh -x -c 'test "$$VAR1" = "word1 word2" && test "$$VAR2" = word3 && test "$$VAR3" = "\\$$word 5 6" && test "$$VAR4" = "new\nline" && test "$$VAR5" = passwordwithbackslashes && test "$$PATH" = "/tmp:/bin"'
Type=oneshot
ExecSearchPaths=/tmp:/bin
EnvironmentFile=/tmp/test-exec_execsearchpaths_environmentfile.conf
