$TTL 86400
$ORIGIN onlinesign.test.

@ IN SOA ns1.signed.test. root.signed.test. (
    42   ; serial
    3H   ; refresh
    15M  ; retry
    1W   ; expire
    1D   ; minimum TTL
)

; NS info
                     NS ns1.signed.test.

; TXT records
                     TXT "hello from onlinesign"

; A/AAAA records
; No A/AAAA record for the $ORIGIN
sub                  A 1.2.3.133
