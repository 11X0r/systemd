; SPDX-License-Identifier: LGPL-2.1-or-later
$TTL 86400
$ORIGIN signed.test.

@ IN SOA ns1.signed.test. root.signed.test. (
    42   ; serial
    3H   ; refresh
    15M  ; retry
    1W   ; expire
    1D   ; minimum TTL
)

; NS info
@                     NS ns1.signed.test.
ns1                   A  1.2.3.1

*.wild                TXT "this is a wildcard"

@                     MX 10 mail.signed.test.

                      A 1.2.3.10
mail                  A 1.2.3.11

; https://github.com/systemd/systemd/issues/22002
dupe                  A 1.2.3.12
dupe                  A 1.2.3.13

cname-chain           CNAME follow1.signed.test.
follow1               CNAME follow2.signed.test.
follow2               CNAME follow3.nested.signed.test.
follow3.nested        CNAME follow4.final.signed.test.
follow4.final         A 1.2.3.14
