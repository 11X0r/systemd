$TTL 86400
$ORIGIN untrusted.test.

@ IN SOA ns1.signed.test. root.signed.test. (
    42   ; serial
    3H   ; refresh
    15M  ; retry
    1W   ; expire
    1D   ; minimum TTL
)

; NS info
@                     NS ns1.signed.test.

*.wild                TXT "this is an untrusted wildcard"

@                     MX 10 mail.untrusted.test.

                      A 1.2.3.121
mail                  A 1.2.3.121
