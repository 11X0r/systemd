$TTL 86400
$ORIGIN dns.systemd.

@ IN SOA primary.dns.systemd. root.dns.systemd. (
    42   ; serial
    3H   ; refresh
    15M  ; retry
    1W   ; expire
    1D   ; minimum TTL
)

; NS info
@                     NS primary.dns.systemd.
primary               A  1.2.3.1

; MX records
@                     MX 10 mail.dns.systemd.
; A/AAAA records
@                     A 1.2.3.10
mail                  A 1.2.3.11
