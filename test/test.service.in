[Unit]
Description=%N
Wants=basic.target network.target
After=basic.target network.target
@unit@

[Service]
ExecStartPre=rm -f /failed /testok
ExecStart=@command@
Type=oneshot
MemoryAccounting=@memory-accounting@
Environment=SYSTEMD_PAGER= @env@
UnsetEnvironment=JOURNAL_STREAM
@service@
